<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Crear record rule para filtrar las compañias - Compras / Recursos económicos -->
        <record id="rule_sre_order_zue_company" model="ir.rule">
            <field name="name">Permisos recursos económicos / Compra</field>
            <field name="model_id" ref="model_sre_order"/>
            <field name="domain_force">['|',('company_id','=',False),('company_id', 'in', company_ids)]</field>                
        </record>
    </data>
</odoo>

