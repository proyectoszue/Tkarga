<odoo>
    <data>
		<record id="view_zue_cost_distribution_form_account_account" model="ir.ui.view">
			<field name="name">zue_cost_distribution.account_account_distribution_inherit</field>
			<field name="model">account.account</field>
			<field name="priority">30</field>
			<field name="inherit_id" ref="account.view_account_form"/>
			<field name="arch" type="xml">                
				<xpath expr="//field[@name='currency_id']" position="after">					
                    <field name="account_distribution"/>					                    
                </xpath>
			</field>
		</record>
    </data>
</odoo>