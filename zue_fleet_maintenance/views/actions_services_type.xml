<odoo>
    <data>
        <record model="ir.actions.act_window" id="action_mntc_services_type">
            <field name="name">Services Type</field>
            <field name="res_model">mntc.services.type</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record model="ir.ui.view" id="zue_fleet_maintenance.list_action_mntc_services_type">
			    <field name="name">zue list mntc services type</field>
			    <field name="model">mntc.services.type</field>
			    <field name="arch" type="xml">
				<tree>
					<field name="name"/>                
					<field name="code"/> 
					<field name="margin"/> 
				</tree>
			</field>
		</record>

        <record model="ir.ui.view" id="zue_fleet_maintenance.form_action_mntc_services_type">
			    <field name="name">zue form mntc services type</field>
			    <field name="model">mntc.services.type</field>
			    <field name="arch" type="xml">
			 <form>
                 <sheet string="Servicios">
                    <group>
                         <field name="name"/>
                        <newline/>
                        <field name="code"/> 
						<newline/>                                
						<field name="margin"/>  
                        <newline/>                                
                    </group>
                 </sheet>
              </form>
			</field>
		</record>

        <record model="ir.actions.act_window" id="action_mntc_location">
            <field name="name">Location</field>
            <field name="res_model">mntc.location</field>
            <field name="view_mode">tree</field>
        </record>

        <record model="ir.ui.view" id="zue_fleet_maintenance.list_mntc_location">
            <field name="name">zue list mntc location</field>
            <field name="model">mntc.location</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>                
                    <field name="location_code"/>                
                    <field name="location_type"/>                
                </tree>
            </field>
        </record>

        <record model="ir.actions.act_window" id="action_mntc_location_dest">
            <field name="name">Location dest</field>
            <field name="res_model">mntc.location.dest</field>
            <field name="view_mode">tree</field>
        </record>

        <record model="ir.ui.view" id="zue_fleet_maintenance.list_mntc_location_dest">
            <field name="name">zue list mntc location dest</field>
            <field name="model">mntc.location.dest</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="spare_part_company_id"/>                
                    <field name="inventory_company_id"/>                
                    <field name="location_dest_name"/>                
                </tree>
            </field>
        </record>
    </data>
</odoo>